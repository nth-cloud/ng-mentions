{
  "name": "@nth-cloud/ng-mentions",
  "version": "10.0.0",
  "description": "Angular components & directives for customizable mentions",
  "repository": {
    "type": "git",
    "url": "https://github.com/nth-cloud/ng-mentions"
  },
  "author": "https://github.com/nth-cloud/ng-mentions/graphs/contributors",
  "license": "MIT",
  "bugs": "https://github.com/nth-cloud/ng-mentions/issues",
  "homepage": "https://github.com/nth-cloud/ng-mentions#readme",
  "engines": {
    "node": ">=12.0.0 || >=14.0.0 || >=16.0.0 || >= 18.0.0",
    "npm": ">=6.5.0 || >=7.0.0 || >=8.0.0"
  },
  "keywords": [
    "angular",
    "mention",
    "mentions",
    "highlight",
    "components"
  ],
  "scripts": {
    "ng": "ng",
    "preinstall": "node misc/preinstall.js",
    "commitmsg": "node misc/validate-commit.js",
    "version": "npm run changelog && git add CHANGELOG.md && npm run update-src-package-version && git add src/package.json",
    "changelog": "conventional-changelog --preset angular --infile CHANGELOG.md --same-file --release-count 1",
    "check-format": "prettier -c .",
    "format": "prettier -w .",
    "build": "npm run lib:build && npm run demo:build",
    "static": "ts-node --project misc/tsconfig.json misc/copy-static-files.ts",
    "update-src-package-version": "ts-node --project misc/tsconfig.json misc/update-src-package-version.ts",
    "lint": "npm run lib:lint",
    "test": "npm run lint && npm run lib:test && npm run schematics:test",
    "ci:test": "npm run check-format && npm run test && npm run build",
    "tdd": "ng test ng-mentions --source-map false",
    "saucelabs": "ng test ng-mentions -c saucelabs",
    "scripts:tdd": "ts-node-dev --respawn --project misc/tsconfig.json node_modules/jasmine/bin/jasmine misc/*.spec.ts",
    "scripts:test": "ts-node --project misc/tsconfig.json node_modules/jasmine/bin/jasmine misc/*.spec.ts",
    "lib:build": "ng build ng-mentions --configuration production && npm run schematics:build && npm run static",
    "lib:lint": "ng lint ng-mentions",
    "lib:test": "ng test ng-mentions --code-coverage --progress false --watch false",
    "schematics:lint": "eslint schematics",
    "schematics:build": "rimraf schematics/dist && rollup --failAfterWarnings -c schematics/rollup.config.js",
    "schematics:tdd": "ts-node-dev --respawn --project misc/tsconfig.json node_modules/jasmine/bin/jasmine schematics/**/*.spec.ts",
    "schematics:test": "ts-node --project misc/tsconfig.json node_modules/jasmine/bin/jasmine schematics/**/*.spec.ts",
    "demo": "npm run demo:docs && npm run demo:stackblitzes && npm run demo:serve",
    "demo:serve": "ng serve demo --host 0.0.0.0",
    "demo:docs": "ts-node --project misc/tsconfig.json misc/generate-docs.ts",
    "demo:lint": "ng lint demo",
    "demo:build": "npm run demo:lint && npm run demo:docs && npm run demo:stackblitzes && ng build demo --configuration production",
    "demo:deploy": "npm run demo:build && npm run demo:publish",
    "demo:publish": "gh-pages --dist demo/dist --branch gh-pages",
    "demo:stackblitzes": "ts-node --project misc/tsconfig.json misc/generate-stackblitzes.ts",
    "clean": "npm run clean:dist & npm run clean:test & npm run clean:demo & npm run clean:demo:deploy && npm run clean:demo:plunks && npm run clean:schematics",
    "clean:dist": "rimraf dist && ([[ ! -e dist.tgz ]] || rm dist.tgz)",
    "clean:test": "rimraf coverage",
    "clean:demo": "rimraf demo/dist",
    "clean:schematics": "rimraf schematics/dist",
    "clean:demo:deploy": "[[ ! -e .publish/ ]] || rm -fr .publish/",
    "clean:demo:plunks": "rimraf demo/src/public/app/components"
  },
  "dependencies": {
    "@angular/common": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "core-js": "~3",
    "rxjs": "^6.6.0 || ^7.0",
    "typescript": "^4.9.5",
    "zone.js": "^0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1501.5",
    "@angular-devkit/build-angular": "^15.1.5",
    "@angular-devkit/core": "^15.1.5",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/animations": "^15.1.4",
    "@angular/cli": "^15.1.5",
    "@angular/compiler": "^15.1.4",
    "@angular/compiler-cli": "^15.1.4",
    "@angular/localize": "^15.1.4",
    "@angular/platform-browser": "^15.1.4",
    "@angular/platform-browser-dynamic": "^15.1.4",
    "@angular/router": "^15.1.4",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.0.0",
    "@schematics/angular": "^15.1.5",
    "@types/estree": "^1.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.0.1",
    "@types/he": "^1.1.0",
    "@types/jasmine": "^4.3.1",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^18.13.0",
    "@types/prismjs": "^1.9.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.44.0",
    "@typescript-eslint/parser": "^5.43.0",
    "bootstrap": "^5.2.3",
    "conventional-changelog-cli": "^2.1.1",
    "ejs": "^3.1.8",
    "eslint": "^8.28.0",
    "eslint-plugin-deprecation": "^1.3.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-jsdoc": "^40.0.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fs-extra": "^11.1.0",
    "gh-pages": "^5.0.0",
    "glob": "^8.1.0",
    "he": "^1.1.0",
    "husky": "^8.0.3",
    "jasmine": "^4.5.0",
    "jasmine-core": "^4.5.0",
    "jasmine-spec-reporter": "^7.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-firefox-launcher": "^2.0",
    "karma-jasmine": "^5.1.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sauce-launcher": "^4.1.5",
    "marked": "^4.0.12",
    "ng-packagr": "^15.1.2",
    "prettier": "^2.8.4",
    "prismjs": "^1.25.0",
    "raw-loader": "^4.0.2",
    "rimraf": "^4.1.2",
    "rollup": "^3.15.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.5.0"
  }
}
